  <trd lng="de">Zwillich</trd>
  <trd lng="en"><klr>(mattress)</klr> ticking</trd>
  <trd lng="fr">coutil</trd>
  <trdgrp lng="hu">
    <trd>csinvat</trd>,
    <trd>zs&aacute;voly</trd>
  </trdgrp>
  <trd lng="nl">tijk</trd>
  <trd lng="ru">&c_t;&c_i;&c_k; <klr
tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
</drv>
</art>
