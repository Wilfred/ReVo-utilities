<drv mrk="cxatni.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxatni.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Hinddevena dol&ccirc;a kaj akra
      <ref tip="super" cel="spic.0ajxo.KUI">spica&jcirc;o</ref>
